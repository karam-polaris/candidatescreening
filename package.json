{
  "name": "candidate-screening",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel -r dev",
    "build": "pnpm -r build",
    "db:migrate": "pnpm --filter @candidate-screening/api db:migrate",
    "db:push": "pnpm --filter @candidate-screening/api db:push",
    "db:studio": "pnpm --filter @candidate-screening/api db:studio",
    "seed": "pnpm --filter @candidate-screening/scripts seed",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
